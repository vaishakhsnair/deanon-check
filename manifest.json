{
    "manifest_version": 2,
    "name": "Tor Deanonymization Checker",
    "version": "1.0",
    "description": "Checks for deanonymization in Tor Browser",
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "storage",
      "notifications",
      "https://check.torproject.org/*"
    ],
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "icons": {
      "48": "icon.png"
    }
  }
  